#include <iostream>
using namespace std;

int main() {
    int sum = 0;

    // For loop to calculate the sum of numbers from 1 to 10
    for (int i = 1; i <= 10; i++) {
        sum += i;  // Add i to sum
    }

    cout << "The sum of the first 10 natural numbers is: " << sum << endl;

    return 0;
}
